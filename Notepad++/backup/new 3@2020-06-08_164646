import numpy as np
import pandas as pd

pd1=pd.DataFrame(np.arange(25).reshape(5,5))
pd2=pd.DataFrame()
print(pd1)
for i in range(len(pd1)):
    a=pd1.loc[i]
    d=pd.DataFrame(a).T
    pd2=pd2.append([d]*5)  #每行复制5倍
print(pd2)


